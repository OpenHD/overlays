/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable PWM9";
		compatible = "radxa,s0";
		category = "misc";
		exclusive = "GPIO2_B3";
		description = "Enable PWM9.\nOn Radxa S0 this is pin 22.";
	};

	fragment@0 {
		target = <&pwm9>;

		__overlay__ {
			status = "okay";
			pinctrl-names = "active";
			pinctrl-0 = <&pwm9_pin>;
		};
	};
};
