/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable PWM6";
		compatible = "radxa,s0";
		category = "misc";
		exclusive = "GPIO0_C2";
		description = "Enable PWM6.\nOn Radxa S0 this is pin 33.";
	};

	fragment@0 {
		target = <&pwm6>;

		__overlay__ {
			status = "okay";
			pinctrl-names = "active";
			pinctrl-0 = <&pwm6_pin>;
		};
	};
};
